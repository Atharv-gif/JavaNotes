// 1st


import java.util.*;

public class Main {

    public static int calculateAverage(int a , int b , int c ){
        int average = (a + b + c)/3 ;
        return average;
    }

    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
            int a = sc.nextInt();
            int b = sc.nextInt();
            int c = sc.nextInt();

            int average = calculateAverage(a, b, c);
            System.out.println(average);
    }
}

// 2nd

import java.util.*;

public class Main {

    public static int sumOfOdds(int n ){
        int sum = 0 ;
        for(int i = 1 ; i <= n ; i += 1){
            if(i%2!=0){
                sum += i;
            }
            
        }
        return sum;

    }

        public static void main(String[] args){
            Scanner sc = new Scanner(System.in);
            int n = sc.nextInt();

            int result = sumOfOdds(n);
            System.out.println(result);
        }
    }

    // 3rd  1st part (did without using a fn)

    import java.util.*;

public class Main {

    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();

        if( a < b ){
            System.out.println("b is greater");
        }
        else{
            System.out.println("a is greater");
        }
    }
}

// 2nd part

import java.util.*;

public class Main {

    public static int greaterNumber(int a , int b){
        if(a<b){
            return b;
        }
        else{
            return a;
        }
    }

    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int a = sc.nextInt();
        int b = sc.nextInt();

        int large = greaterNumber(a, b);
        System.out.println(large);
    }

}
 

// 4th 


import java.util.*;

public class Main {

    public static double circleCircumference(int r) {
        double circumference = 2 * 3.14 * r;
        return circumference;
    }

    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int r = sc.nextInt();

        double answer = circleCircumference(r);
        System.out.println("Circumference is: " + answer);
    }
}


// 5th 

import java.util.*;

public class Main {

    public static void peopleAge(int n ){  // void isleye likha cus ye kuch bhi return kardega 
        if(n < 18){
            System.out.println("not eligible"); // you can also write as , return True or return false 
        }
        else{
            System.out.println("eligible");
        }
    }

    public static void main(String[] args){
        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();

        int age = peopleAge(n);
        System.out.println(age);
    }
}



import java.util.*; // ARRAY wala problem 

public class ArrayExample {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);

        int rows = sc.nextInt(); 
        int cols = sc.nextInt(); 

        int[][] numbers = new int[rows][cols]; 

        // input values
        for (int i = 0; i < rows; i++) {
            for (int j = 0; j < cols; j++) { 
                numbers[i][j] = sc.nextInt();
            }
        }

        // output values
        for (int i = 0; i < rows; i++) {
            for (int j = 0; j < cols; j++) {
                System.out.print(numbers[i][j] + " "); 
            }
            System.out.println(); 
        }

        sc.close(); 
    }
}



import java.util.*;

public class ArrayExample{
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        int rows = sc.nextInt();
        int cols = sc.nextInt();

        int[][] numbers = new int[rows][cols];


        for(int i = 0 ; i < rows ; i++){
            for(int j = 0 ; j < cols ; j++){
                numbers[rows][cols] = sc.nextInt();
            }
        }

        int x = sc.nextInt();

        for(int i = 0; i < rows ; i++){
            for(int j = 0 ; j < cols ; j++){

                if(numbers[rows][cols] == x){
                    System.out.println("x found at location (" + i + " ," + j + ")");
                }
            }
        }
    }
}


// strings 

import java.util.Scanner;

public class Strings {

    public static void main(String[] args){

        String firstname = "peter";
        String lastname = "parker";
        String fullname = firstname + " " + lastname;
        System.out.println(fullname.length()); // .length total length batata hai string ka 

        for(int i = 0 ; i <fullname.length(); i++){
            System.out.println(fullname.charAt(i)); // char at har character ko indv.. likhta hai 
        }

        
    }
}


import java.util.Scanner;

public class Strings {

    public static void main(String[] args){

        String name1 = "peter";
        string name2 = "peter";

        // cases jo ye karega abhi compare krte waqt it wil follow three cases 
        // if s1 > s2 then postive value 
        // if s1 == s2 , then 0 value 
        // if s1 < s2 , then negative value 

        // agar strings dono same length ke hai to jo characters unke compare hoga , i mean pehle character dekhnege and then doosra if req , jo jitta bada uska utta preference

        if(name1.compareTo(name2) == 0 ) {
            System.out.println("Strings are equal");
        } else {
            System.out.println("strings are not equal");
        }

        
    }
}


import java.util.*;

public class Strings {
    public static void main(String args[]) {

        // The substring() method in Java is used to extract a part of a string.
        // Syntax: string.substring(startIndex, endIndex)
        // - It returns characters from startIndex up to (but not including) endIndex.

        String sentence = "My name is Tony"; // The original sentence
        // Index positions:  0123456789.... (spaces count too)

        // Let's extract the name "Tony"
        // 'T' is at index 11, and we want to go till the end of the string
        String name = sentence.substring(11, sentence.length());

        System.out.println(name); // Output will be: Tony
    }
}


